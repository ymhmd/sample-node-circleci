{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": false,
    "removeComments": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "ConvertedJSFiles",
    "types": ["jasmine", "node"]
  },
  "exclude": [
    "node_modules"
  ]
}